    "def insertion_sort(arr):\n",
    "    for i in range(1, len(arr)):\n",
    "        anchor = arr[i]\n",
    "        j = i - 1\n",
    "        \n",
    "        while j >= 0 and anchor < arr[j]:\n",
    "            arr[j+1] = arr[j]\n",
    "            j -= 1\n",
    "        arr[j+1] = anchor\n",
    "    return arr"
   ]
