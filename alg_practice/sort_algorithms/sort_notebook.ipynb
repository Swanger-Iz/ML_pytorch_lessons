   "metadata": {},
   "outputs": [],
   "source": [
    "def bubble_sort(arr):\n",
    "    size = len(arr)\n",
    "    swapped = False\n",
    "    \n",
    "    while size != 0 or not swapped:\n",
    "        for i in range(size-1):\n",
    "            if arr[i] > arr[i+1]: \n",
    "                arr[i], arr[i+1] = arr[i+1], arr[i]\n",
    "                swapped = True\n",
    "         \n",
    "        if not swapped: break     \n",
    "        size -= 1\n",
    "    \n",
    "    return arr"
   ]
