    "def bb_sort(arr, key):\n",
    "    size = len(arr)\n",
    "    swapped = False\n",
    "    \n",
    "    while size != 0:\n",
    "        for i in range(size-1):\n",
    "            if arr[i][key] > arr[i+1][key]:\n",
    "                temp = arr[i]\n",
    "                arr[i] = arr[i+1]\n",
    "                arr[i+1] = temp\n",
    "                \n",
    "                swapped = True\n",
    "        \n",
    "        if not swapped: break\n",
    "        size -= 1\n",
    "    return arr"
   ]
